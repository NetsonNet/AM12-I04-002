<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dispositivos.pluspalexamen.clases.moviles.nels.am12_i04_002">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".ActividadDepto">
        <intent-filter>
            <action android:name="android.intent.action.ACTIVIDADDEPTO" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>


        <activity android:name=".ActividadMunicipio">
            <intent-filter>
                <action android:name="android.intent.action.ACTIVIDADMUNICIPIO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>